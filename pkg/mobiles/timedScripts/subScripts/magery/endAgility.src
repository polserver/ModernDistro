use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndAgility(params)
	var mobile := params[1];
	
	var points := CInt(GetObjProperty(mobile, "AgilityMod"));	
	AP_ModifyStatMod(mobile, DEXTERITY, -(points));
	EraseObjProperty(mobile, "AgilityMod");
	
	return 1;
endprogram

