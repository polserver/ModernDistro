use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndWeaken(params)
	var mobile := params[1];

	var points := CInt(GetObjProperty(mobile, "WeakenMod"));	
	AP_ModifyStatMod(mobile, STRENGTH, points);
	EraseObjProperty(mobile, "WeakenMod");
	
	return 1;
endprogram

