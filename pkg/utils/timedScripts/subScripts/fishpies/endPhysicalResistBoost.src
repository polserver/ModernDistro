use uo;
use os;

include ":attributes:attributes";
include ":damage:damage";

program TimedScript_endPhysicalResistBoost(params)
	var mobile := params[1];
	var points := CInt(GetObjProperty(mobile, "FishPhysicalResistBoost"));
	points := -1 * points;
	ModifyResistance(mobile, DMG_PHYSICAL, points);
	EraseObjProperty(mobile, "FishPhysicalResistBoost");

	return 1;
endprogram

