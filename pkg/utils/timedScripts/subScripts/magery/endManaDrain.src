use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndManaDrain(params)
	var mobile := params[1];

	var points := CInt(GetObjProperty(mobile, "#ManaDrainMod"));	
	AP_SetVital(mobile, MANA, AP_GetVital(mobile, MANA) + points);	
	
	return 1;
endprogram

