function GetHumanDamagedSounds( mobile )

	var damagedSound;
	case( mobile.gender )
		0: // Male
			damagedSound := array{341, 342, 343, 345, 346};
			break;
		1: //Female
			damagedSound := array{332, 333, 334, 335, 336};
			break;
	endcase

	return damagedSound.RandomEntry();
endfunction
